#ifndef __CCWRAP_STRING_VIEW_INCLUDED
#define __CCWRAP_STRING_VIEW_INCLUDED

//#include <ccwrap_header.h>
#include <cstddef>

#ifndef __CCWRAP_STD
 #define __CCWRAP_STD    std
#endif

#if !defined __CCWRAP__
 #if defined(__has_include)
  #if __has_include(<../include/string_view>)
   #include <../include/string_view>
   #define __CCWRAP_STRING_VIEW_DEFINED
  #endif
 #endif
#endif

#if !defined __CCWRAP_STRING_VIEW_DEFINED
 #if defined __CCWRAP__ && !defined(__CCWRAP_NO_HEADER_STRING_VIEW)
  #ifdef __CCWRAP_NATIVE_STL_HEADER_PATH
   #include __CCWRAP_NATIVE_STL_HEADER_PATH(string_view)
   #define __CCWRAP_STRING_VIEW_DEFINED
  #elif defined(__CCWRAP_HAS_INCLUDE_NEXT)
   #include_next <string_view>
   #define __CCWRAP_STRING_VIEW_DEFINED
  #else
   #include <../include/string_view>
   #define __CCWRAP_STRING_VIEW_DEFINED
  #endif
 #endif
#endif

#if !defined __CCWRAP_STRING_VIEW_DEFINED
 #if defined __CCWRAP_BOOST2STD
  #include "../boost2std/string_view"
  #define __CCWRAP_STRING_VIEW_DEFINED
 #elif (!defined __CCWRAP__ || defined __CCWRAP_NO_HEADER_STRING_VIEW)
  #include "../_ccwrap/string_view.hpp"
  #define __CCWRAP_STRING_VIEW_DEFINED
 #endif
#endif

#endif      //  __CCWRAP_STRING_VIEW_INCLUDED
