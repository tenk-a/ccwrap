#ifndef __CCWRAP_CERRNO_INCLUDED
#define __CCWRAP_CERRNO_INCLUDED

#include <boost2std_common.hpp>

#include <errno.h>

#if defined(__CCWRAP_HAS_INCLUDE_NEXT)
  #include_next <cerrno>
#elif defined(__CCWRAP_NATIVE_C_HEADER_PATH)
  #include __CCWRAP_NATIVE_C_HEADER_PATH(cerrno)
#else
  #include <../include/cerrno>
#endif

#ifndef errno
#define errno	errno
#endif

#endif
